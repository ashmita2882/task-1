#include<iostream>
using namespace std;
int main ()
{
    int arr[10],n,i,sum=0;
    cout<<"enter the size of the array :";
    cin>>n;
    cout<<"enter the elements of the array :";
     for (i=0;i<n;i++)
    cin>>arr[i];
    for (i=0;i<n;i++)
    {
        sum+= arr[i];
    }
    cout<<"the sum of the elements is :"<<sum;
    return 0;
}

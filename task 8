#include<iostream>
using namespace std;
int main{
    char str1[10],str2[10];
gets (str1);
gets (str2);
int i,j;
int n1=strlen(str1);
int n2=strlen (str2);
int c=0;
if (n1!=n2)
{
    cout<<"they are not anagram";
    return 0;
}
else
{
    for(i=0;i<n1;i++)
    for(j=0;j<n2;j++)
    if(str1[i]==str2[j])
}
if (c==n1)
cout<<"they are an anagram.";
else
cout<<"they are NOT an anagram.";
return 0;
}
